            "parameters": [
                {
                    "name": "angle",
                    "type": "float",
                    "description": "angle in radians"
                }
            ],
            "returnValue": {
                "type": "float",
                "description": "tangent of the angle"
            }
        },
        {
            "kind": "function",
            "name": "asin",
            "description": {
                "p": ["Returns the arcsine of the given value."]
            },
            "parameters": [
                {
                    "name": "value",
                    "type": "float",
                    "description": "value between -1 and 1"
                }
            ],
            "returnValue": {
                "type": "float",
                "description": "angle in radians"
            }
        },
        {
            "kind": "function",
            "name": "acos",
            "description": {
                "p": ["Returns the arccosine of the given value."]
            },
            "parameters": [
                {
                    "name": "value",
                    "type": "float",
                    "description": "value between -1 and 1"
                }
            ],
            "returnValue": {
                "type": "float",
                "description": "angle in radians"
            }
        },
        {
            "kind": "function",
            "name": "radians",
            "description": {
                "p": ["Converts the given angle from degrees to radians."]
            },
            "parameters": [
                {
                    "name": "angle",
                    "type": "float",
                    "description": "angle in degrees"
                }
            ],
            "returnValue": {
                "type": "float",
                "description": "angle in radians"
            }
        },
        {
            "kind": "function",
            "name": "log",
            "description": {
                "p": ["Returns the logarithm of the given value.", "Unless a different base is provided, computes the natural logarithm."]
            },
            "parameters": [
                {
                    "name": "x",
                    "type": "float",
                    "description": "value to compute the logarithm of"
                },
                {
                    "name": "base",
                    "type": "float",
                    "description": "base of the logarithm",
                    "default": "e"
                }
            ],
            "returnValue": {
                "type": "float",
                "description": "logarithm of the provided value"
            }
        },
        {
            "kind": "function",
