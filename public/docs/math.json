                {
                    "name": "x",
                    "type": "float",
                    "description": "number to compute the square root of"
                }
            ],
            "returnValue": {
                "type": "float",
                "description": "square root of the provided number"
            }
        },
        {
            "kind": "function",
            "name": "exp",
