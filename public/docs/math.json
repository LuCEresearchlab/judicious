{
    "module": "math",
    "summary": "Basic mathematical functions and constants",
    "elements": [
        {
            "kind": "function",
            "name": "sin",
            "description": {
                "p": ["Returns the sine of the given angle."]
            },
            "parameters": [
                {
                    "name": "angle",
                    "type": "float",
                    "description": "angle in radians"
                }
            ],
            "returnValue": {
                "type": "float",
                "description": "sine of the angle"
            }
        },
        {
            "kind": "function",
            "name": "cos",
            "description": {
                "p": ["Returns the cosine of the given angle."]
            },
            "parameters": [
                {
                    "name": "angle",
                    "type": "float",
                    "description": "angle in radians"
                }
            ],
            "returnValue": {
                "type": "float",
                "description": "cosine of the angle"
            }
        },
        {
            "kind": "function",
            "name": "tan",
            "description": {
                "p": ["Returns the tangent of the given angle."]
